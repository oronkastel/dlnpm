{
    "name": "dlnpm",
    "version": "0.0.1",
    "description": "Download npm packages as .tgz files in order to be used in private npm repository",
    "main": "lib/index.js",
    "types": "lib/index.d.ts",
    "scripts": {
        "start": "nodemon --watch **/*.ts --exec ts-node src/index.ts",
        "build": "tsc -p .",
        "test": "echo \"Error: no test specified\" && exit 1",
        "prepublish": "tsc -p .",
        "refresh": "rm -rf ./node_modules ./package-lock.json && npm install"
    },
    "keywords": [
        "npm",
        "install",
        "repository",
        "nexus"
    ],
    "author": "Oron Kastel-Mizrahi <oron.kastel@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/jest": "^22.2.3",
        "@types/node": "^12.12.2",
        "jest": "^24.9.0",
        "nodemon": "^2.0.2",
        "ts-jest": "^24.2.0",
        "ts-node": "^8.5.4",
        "typescript": "^3.4.3"
    },
    "dependencies": {
        "chalk": "^3.0.0",
        "clear": "^0.1.0",
        "commander": "^4.0.1",
        "figlet": "^1.2.4",
        "node-fetch": "^2.6.0",
        "path": "^0.12.7",
        "semver": "^7.1.1"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/oronkastel/dlnpm.git"
    },
    "bin": {
        "dlnpm": "lib/index.js"
    },
    "configurations": [
        {
            "type": "node"
        }
    ]
}
